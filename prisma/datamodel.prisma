type User {
    id: ID! @id
    username: String!
    email: String!
    image_url: String!
    phone: String!
    first_name: String!
    last_name: String!
    room_no: String!
}

type Washer {
    id: ID! @id
    status: String!
    time_elapsed: String!
    time_remaining: String!
}

type Society {
    id: ID! @id
    name: String!
    members: [User]!
}