type User {
    id: ID! @id
    username: String! @unique
    email: String! @unique
    image_url: String!
    phone: String! @unique
    first_name: String!
    last_name: String!
    room_no: String! @unique
}

type Washer {
    id: ID! @id
    status: String!
    time_elapsed: String!
    time_remaining: String!
}

type Room {
    number: Int! @id
    name: String!
}

type Booking {
    id: ID! @id
    user: User!
    room: Room!
    start: DateTime!
    end: DateTime!
    createdAt: DateTime! @createdAt
}